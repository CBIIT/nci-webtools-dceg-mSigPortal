            },
            "value": "COSMIC_v3_Signatures_GRCh37_SBS96"
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Limit response size",
            "required": false,
            "schema": {
              "type": "string"
            },
            "value": "10"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          },
          "500": {
            "description": "API error"
          }
        }
      }
    },
