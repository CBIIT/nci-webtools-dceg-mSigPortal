{
  "openapi": "3.0.0",
  "tags": [
    {
      "name": "Visualization"
    },
    {
      "name": "Exploration"
    },
    {
      "name": "Signature"
    }
  ],
  "paths": {
    "/api/seqmatrix": {
      "get": {
        "tags": ["Visualization"],
        "summary": "Retrieves seqmatrix data",
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "study",
            "in": "query",
            "description": "Select which study to query data from.",
            "required": true,
            "schema": {
              "type": "string",
              "enum": ["PCAWG"]
            },
            "value": "PCAWG"
          },
          {
            "name": "cancer",
            "in": "query",
            "description": "Select cancer belonging to the study",
            "required": true,
            "schema": {
              "type": "string"
            },
            "value": "Lung-AdenoCA"
          },
          {
            "name": "strategy",
            "in": "query",
            "description": "Select strategy (dataset) of the study.",
            "required": true,
            "schema": {
              "type": "string",
              "enum": ["WGS"]
            },
            "value": "WGS"
          },
          {
            "name": "sample",
            "in": "query",
            "description": "Select sample.",
            "required": false,
            "schema": {
              "type": "string"
            },
            "value": "SP50263"
          },
          {
            "name": "profile",
            "in": "query",
            "description": "Select a profile type to filter data by.",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["SBS", "DBS", "ID"]
            },
            "value": "SBS"
          },
          {
            "name": "matrix",
            "in": "query",
            "description": "Select a matrix size that coresponds to the profile.",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["96", "83", "78"]
            },
            "value": "96"
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Limit response size",
            "required": false,
            "schema": {
              "type": "string"
            },
            "value": "10"
          }
        ],
        "responses": {
          "200": {
            "description": "Query Signature Data"
          },
          "500": {
            "description": "API error"
          }
        }
      }
    },
    "/api/exposure": {
      "get": {
        "tags": ["Exploration"],
        "summary": "Retrieves Exposure data",
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "study",
            "in": "query",
            "description": "Select which study to query data from.",
            "required": true,
            "schema": {
              "type": "string",
              "enum": ["PCAWG"]
            },
            "value": "PCAWG"
          },
          {
            "name": "strategy",
            "in": "query",
            "description": "Select strategy (dataset) to filter data by.",
            "required": true,
            "schema": {
              "type": "string",
              "enum": ["WGS"]
            },
            "value": "WGS"
          },
          {
            "name": "signatureSetName",
            "in": "query",
            "description": "Select signature set to filter data by.",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "COSMIC_v3_Signatures_GRCh37_SBS96",
                "COSMIC_v3_Signatures_GRCh37_DBS78",
                "COSMIC_v3_Signatures_GRCh37_ID83"
              ]
            },
            "value": "COSMIC_v3_Signatures_GRCh37_SBS96"
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Limit response size",
            "required": false,
            "schema": {
              "type": "string"
            },
            "value": "10"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          },
          "500": {
            "description": "API error"
          }
        }
      }
    },
