            "required": true,
            "schema": {
              "type": "string",
              "enum": ["96", "78", "83"]
            },
            "value": "96"
          },
          {
            "name": "signatureSetName",
            "in": "query",
            "description": "Select signature set to filter data by.",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "",
                "COSMIC_v3_Signatures_GRCh37_SBS96",
                "COSMIC_v3_Signatures_GRCh37_DBS78",
                "COSMIC_v3_Signatures_GRCh37_ID83"
